{
    "extends": [
      "next/core-web-vitals",
      "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error", { "endOfLine": "auto" }],
      "react/jsx-curly-brace-presence": ["error", { "props": "never", "children": "never" }]
    }
  }